{
  "name": "zacksleo/yii2-ad",
  "description": "yii2 ad manage module",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "zacksleo",
      "email": "zacksleo@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "npm-asset/bootstrap-datetime-picker": "^2.4",
    "nemmo/yii2-attachments": "^1.0.0-beta.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "zacksleo\\yii2\\ad\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "zacksleo\\yii2\\ad\\tests\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}